#!/bin/sh
if [ $# -le 1 ] ; then
	echo "$0 nmemb <option of Sort command>"
else
	rm  tmp* 2>/dev/null
	N=$1; shift
	for i in `seq 1 10`; do
		src/random.awk $N | Release/Sort -N $N -Z 64 $*; sleep 1
	done | grep -v qsort.3 | tee spread.tmp |
	awk '{print $1}' | xargs printf "%s\t"
	awk '{print $4}' spread.tmp | xargs src/esd.awk	
fi
